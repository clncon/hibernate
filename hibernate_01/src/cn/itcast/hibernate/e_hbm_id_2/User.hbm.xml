<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hibernate.test.any">

    <class name="cn.itcast.hibernate.e_hbm_id_2.User" table="user">
      
         
	      <composite-id>
			<key-property name="firstName" type="string"></key-property>
			<key-property name="lastName" type="string"></key-property>
		</composite-id>
	   
	<property name="gender"/>
    </class>
</hibernate-mapping>
